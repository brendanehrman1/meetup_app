<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.planowestapp1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_meetup"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_meetup_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChangePasswordActivity"></activity>
        <activity android:name=".CalendarActivity" />
        <activity android:name=".AddTimeActivity" />
        <activity android:name=".TimeDataActivity" />
        <activity android:name=".TimesActivity" />
        <activity android:name=".AddFriendActivity" />
        <activity android:name=".FriendDataActivity" />
        <activity android:name=".FriendActivity" />
        <activity android:name=".CreateAccountActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>